#include <iostream>
#include <set>

void Elements(const std::set<int>& setA, const std::set<int>& setB) {
    bool foundCommon = false;
    std::cout << "Общие положительные элементы: ";

 
    for (int elem : setA) {
       
        if (elem > 0 && setB.find(elem) != setB.end()) {
            std::cout << elem << " ";
            foundCommon = true;
        }
    }

    if (!foundCommon) {
        std::cout << "таких элементов нет";
    }
    std::cout << std::endl;
}

int main() {
    int N;
    std::cout << "Введите размер множеств (N): ";
    std::cin >> N;

    std::set<int> A, B;

    std::cout << "Введите " << N << " элементов для множества A:" << std::endl;
    for (int i = 0; i < N; i++) {
        int value;
        std::cin >> value;
        A.insert(value);
    }


    std::cout << "Введите " << N << " элементов для множества B:" << std::endl;
    for (int i = 0; i < N; i++) {
        int value;
        std::cin >> value;
        B.insert(value);
    }

    Elements(A, B);

    return 0;
}

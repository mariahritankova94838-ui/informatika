#include <iostream>
#include <set>
#include <algorithm>   
#include <iterator>    

void Elements(const std::set<int>& setA, const std::set<int>& setB) {
    
    std::set<int> positiveA;
    for (int elem : setA) {
        if (elem > 0) {
            positiveA.insert(elem);
        }
    }

    std::set<int> positiveB;
    for (int elem : setB) {
        if (elem > 0) {
            positiveB.insert(elem);
        }
    }

    
    std::set<int> result;
    std::set_intersection(positiveA.begin(), positiveA.end(),
        positiveB.begin(), positiveB.end(),
        std::inserter(result, result.begin()));

    std::cout << "Общие положительные элементы: ";
    if (result.empty()) {
        std::cout << "таких элементов нет";
    }
    else {
        for (int elem : result) {
            std::cout << elem << " ";
        }
    }
    std::cout << std::endl;
}


int main() {
    int N;
    std::cout << "Введите размер множеств (N): ";
    std::cin >> N;

    std::set<int> A, B;

    std::cout << "Введите " << N << " элементов для множества A:" << std::endl;
    for (int i = 0; i < N; i++) {
        int value;
        std::cin >> value;
        A.insert(value);
    }

    std::cout << "Введите " << N << " элементов для множества B:" << std::endl;
    for (int i = 0; i < N; i++) {
        int value;
        std::cin >> value;
        B.insert(value);
    }

    Elements(A, B);
    return 0;
